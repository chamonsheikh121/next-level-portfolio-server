model ProjectType {
  id       Int       @id @default(autoincrement())
  title    String    @unique
  projects Project[]
}

model Project {
  id                   Int         @id @default(autoincrement())
  title                String
  subtitle             String?
  imageURL             String?
  typeId               Int
  type                 ProjectType @relation(fields: [typeId], references: [id], onDelete: Cascade)
  frontendTechs        String[]
  backendTechs         String[]
  devopsTechs          String[]
  designTechs          String[]
  othersTechs          String[]
  keyAccomplishments   String[]
  projectOverview      String?
  problems             Json
  solutions            Json
  solutionArchitecture Json
  challenges           Json
  timeline             String?
  role                 String?
  totalMemberWorked    Int?
  outcome              String?
}
