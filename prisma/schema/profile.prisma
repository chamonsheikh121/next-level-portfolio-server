model ProfileInformation {
  id              Int             @id @default(autoincrement())
  name            String
  subtitle        String?
  imageURL        String?
  location        String?
  bio             String?
  description     String?
  resumeURL       String?
  contactEmail    String?
  phone           String?
  workingHour     String?
  status          DeveloperStatus @default(AVAILABLE)
}


enum DeveloperStatus {
  AVAILABLE
  LIMITED
  IN_PROGRESS
  FULLY_BOOKED
  ON_VACATION
  NOT_AVAILABLE
}